	urldate = {2015-09-28},
	journal = {Journal of electrocardiology},
	author = {Strauss, David G and Sprague, Paula Quintal and Underhill, Kevin and Maynard, Charles and Adams, George L and Kessenich, Amy and Sketch, Michael H and Berger, Peter B and Marcozzi, David and Granger, Christopher B and Wagner, Galen S},
