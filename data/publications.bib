	note = {Publisher: American College of Cardiology Foundation},
	keywords = {Emergency Medical Services, Electrocardiography, Female, Humans, Male, Middle Aged, Aged, Myocardial Infarction, Registries, Time Factors, Myocardial Infarction: therapy, Angioplasty, Balloon, Coronary, Myocardial Reperfusion, Fibrinolytic Agents, Fibrinolytic Agents: therapeutic use},
