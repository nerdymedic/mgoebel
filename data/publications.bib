@article{bradley_strategies_2006,
	title = {Strategies for reducing the door-to-balloon time in acute myocardial infarction.},
	volume = {355},
	issn = {1533-4406},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17101617},
	doi = {10.1056/NEJMsa063117},
	abstract = {BACKGROUND: Prompt reperfusion treatment is essential for patients who have myocardial infarction with ST-segment elevation. Guidelines recommend that the interval between arrival at the hospital and intracoronary balloon inflation (door-to-balloon time) during primary percutaneous coronary intervention should be 90 minutes or less. However, few hospitals meet this objective. We sought to identify hospital strategies that were significantly associated with a faster door-to-balloon time.

METHODS: We surveyed 365 hospitals to determine whether each of 28 specific strategies was in use. We used hierarchical generalized linear models and data on patients from the Centers for Medicare and Medicaid Services to determine the association between hospital strategies and the door-to-balloon time.

RESULTS: In multivariate analysis, six strategies were significantly associated with a faster door-to-balloon time. These strategies included having emergency medicine physicians activate the catheterization laboratory (mean reduction in door-to-balloon time, 8.2 minutes), having a single call to a central page operator activate the laboratory (13.8 minutes), having the emergency department activate the catheterization laboratory while the patient is en route to the hospital (15.4 minutes), expecting staff to arrive in the catheterization laboratory within 20 minutes after being paged (vs. {\textgreater}30 minutes) (19.3 minutes), having an attending cardiologist always on site (14.6 minutes), and having staff in the emergency department and the catheterization laboratory use real-time data feedback (8.6 minutes). Despite the effectiveness of these strategies, only a minority of hospitals surveyed were using them.

CONCLUSIONS: Several specific hospital strategies are associated with a significant reduction in the door-to-balloon time in the management of myocardial infarction with ST-segment elevation.},
	number = {22},
	urldate = {2015-09-06},
	journal = {The New England journal of medicine},
	author = {Bradley, Elizabeth H and Herrin, Jeph and Wang, Yongfei and Barton, Barbara A and Webster, Tashonna R and Mattera, Jennifer A and Roumanis, Sarah A and Curtis, Jeptha P and Nallamothu, Brahmajee K and Magid, David J and McNamara, Robert L and Parkosewich, Janet and Loeb, Jerod M and Krumholz, Harlan M},
	month = nov,
	year = {2006},
	pmid = {17101617},
	keywords = {Emergency Medical Services, Humans, Myocardial Infarction, Time Factors, Myocardial Infarction: therapy, Emergency Medical Services: standards, Emergency Service, Hospital, Angioplasty, Balloon, Coronary, Coronary: standards, Multivariate Analysis, Emergency Medical Services: organization \& adminis, Hospital: organization \& admini, Cross-Sectional Studies, Hospitals, Linear Models},
	pages = {2308--20},
	file = {bradley_2006_strategies_for_reducing_the_door-to-balloon_time_in_acute_myocardial_infarction.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/The New England journal of medicine/2006/bradley_2006_strategies_for_reducing_the_door-to-balloon_time_in_acute_myocardial_infarction.pdf:application/pdf},
