Publisher: Taylor \& Francis
ISBN: 0003-1305},
	keywords = {Bioequivalence testing, Burden of proof, Observed power, Retrospective power analysis, Statistical power, Type II error},
	pages = {19--24},
	file = {hoenig_2001_the_abuse_of_power_-_the_pervasive_fallacy_of_power_calculations_for_data.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/American Statistician/2001/hoenig_2001_the_abuse_of_power_-_the_pervasive_fallacy_of_power_calculations_for_data.pdf:application/pdf},
