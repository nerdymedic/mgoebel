	author = {Hasselqvist-Ax, Ingela and Riva, Gabriel and Herlitz, Johan and Rosenqvist, Mårten and Hollenberg, Jacob and Nordberg, Per and Ringh, Mattias and Jonsson, Martin and Axelsson, Christer and Lindqvist, Jonny and Karlsson, Thomas and Svensson, Leif and Ph, D and Hollenberg, Jacob and Ph, D},
	month = jun,
	year = {2015},
	pmid = {26061835},
	keywords = {Emergency Medical Services, Adult, Female, Humans, Male, Middle Aged, Aged, 80 and over, Cardiopulmonary Resuscitation, Out-of-Hospital Cardiac Arrest, Out-of-Hospital Cardiac Arrest: complications, Out-of-Hospital Cardiac Arrest: mortality, Out-of-Hospital Cardiac Arrest: therapy, Propensity Score, Survival Rate, Sweden, Sweden: epidemiology, Telemedicine, Telephone, Time-to-Treatment},
	pages = {2307--15},
