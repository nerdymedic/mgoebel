	keywords = {Cardiac arrest, Extracorporeal life support, Adult, Emergency Service, Hospital, Female, Humans, Male, Middle Aged, Aged, Registries, Treatment Outcome, Prospective Studies, Cardiopulmonary Resuscitation, Out-of-Hospital Cardiac Arrest, Confidence Intervals, Follow-Up Studies, Risk Assessment, Survival Analysis, Hospital Mortality, Extracorporeal Membrane Oxygenation, Heart Arrest, ECMO, Academic Medical Centers, ECLS, E-CPR, Extracorporeal membrane oxyganation, Life Support Systems, Philadelphia},
	pages = {1527--1532},
