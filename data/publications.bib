	journal = {The Lancet},
	author = {Yannopoulos, Demetris and Bartos, Jason and Raveendran, Ganesh and Walser, Emily and Connett, John and Murray, Thomas A. and Collins, Gary and Zhang, Lin and Kalra, Rajat and Kosmopoulos, Marinos and John, Ranjit and Shaffer, Andrew and Frascone, R. J. and Wesley, Keith and Conterato, Marc and Biros, Michelle and Tolar, Jakub and Aufderheide, Tom P.},
	month = nov,
	year = {2020},
