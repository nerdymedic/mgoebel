	file = {zegre-hemsey_2015_normal_prehospital_electrocardiography_is_linked_to_long-term_survival_in.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Journal of Electrocardiology/2015/zegre-hemsey_2015_normal_prehospital_electrocardiography_is_linked_to_long-term_survival_in.pdf:application/pdf},
