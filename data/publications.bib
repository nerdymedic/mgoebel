@article{ebell_accuracy_2020,
	title = {Accuracy of {Signs} and {Symptoms} for the {Diagnosis} of {Community}‐acquired {Pneumonia}: {A} {Meta}‐analysis},
	volume = {27},
	issn = {1069-6563, 1553-2712},
	shorttitle = {Accuracy of {Signs} and {Symptoms} for the {Diagnosis} of {Community}‐acquired {Pneumonia}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/acem.13965},
	doi = {10.1111/acem.13965},
	abstract = {Background: Community-acquired pneumonia (CAP) is an important source of morbidity and mortality. However, overtreatment of acute cough illness with antibiotics is an important problem, so improved diagnosis of CAP could help reduce inappropriate antibiotic use.
Methods: This was a meta-analysis of prospective cohort studies of patients with clinically suspected pneumonia or acute cough that used imaging as the reference standard. All studies were reviewed in parallel by two researchers and quality was assessed using the QUADAS-2 criteria. Summary measures of accuracy included sensitivity, speciﬁcity, likelihood ratios, the diagnostic odds ratio, and the area under the receiver operating characteristic curve (AUROCC) and were calculated using bivariate meta-analysis.
Results: We identiﬁed 17 studies, of which 12 were judged to be at low risk of bias and the remainder at moderate risk of bias. The prevalence of CAP was 10\% in nine primary care studies and was 20\% in seven emergency department studies. The probability of CAP is increased most by an abnormal overall clinical impression suggesting CAP (positive likelihood ratio [LR+] = 6.32, 95\% CI = 3.58 to 10.5), egophony (LR+ = 6.17, 95\% CI = 1.34 to 18.0), dullness to percussion (LR+ = 2.62, 95\% CI = 1.14 to 5.30), and measured temperature (LR+ = 2.52, 95\% CI = 2.02 to 3.20), while it is decreased most by the absence of abnormal vital signs (LRÀ = 0.25, 95\% CI = 0.11 to 0.48). The overall clinical impression also had the highest AUROCC at 0.741.
Conclusions: While most individual signs and symptoms were unhelpful, selected signs and symptoms are of value for diagnosing CAP. Teaching and performing these high value elements of the physical examination should be prioritized, with the goal of better targeting chest radiographs and ultimately antibiotics.},
	language = {en},
	number = {7},
	urldate = {2020-07-24},
	journal = {Academic Emergency Medicine},
	author = {Ebell, Mark H. and Chupp, Hulme and Cai, Xinyan and Bentivegna, Michelle and Kearney, Maggie},
	editor = {Zehtabchi, Shahriar},
	month = jul,
	year = {2020},
	pages = {541--553},
	file = {ebell_2020_accuracy_of_signs_and_symptoms_for_the_diagnosis_of_community‐acquired.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Academic Emergency Medicine/2020/ebell_2020_accuracy_of_signs_and_symptoms_for_the_diagnosis_of_community‐acquired.pdf:application/pdf},
}

