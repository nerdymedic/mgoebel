@article{baggish_decade_2015,
	title = {A decade of athlete {ECG} criteria: {Where} we’ve come and where we’re going},
	volume = {48},
	issn = {00220736},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/25771161},
	doi = {10.1016/j.jelectrocard.2015.02.002},
	abstract = {Competitive athletes and highly active individuals commonly develop cardiovascular adaptations. Underlying features of exercise-induced cardiac remodeling often manifest as distinct patterns on the 12-lead electrocardiogram (ECG). Considerable effort has been directed toward defining the scope of ECG abnormalities in this population and numerous criteria have been proposed for the task of differentiating benign athletic ECG changes from patterns suggestive of underlying cardiovascular disease. This review will provide a historical overview of athlete ECG criteria development with a final emphasis on remaining areas of scientific and clinical uncertainty.},
	number = {3},
	urldate = {2015-11-09},
	journal = {Journal of Electrocardiology},
	author = {Baggish, Aaron L.},
	month = jan,
	year = {2015},
	pmid = {25771161},
	note = {Publisher: Elsevier Inc.},
