	author = {Kukla, Piotr and McIntyre, William F and Fijorek, Kamil and Mirek-Bryniarska, Ewa and Bryniarski, Leszek and Krupa, Ewa and Jastrzębski, Marek and Bryniarski, Krzysztof L and Zhong-qun, Zhan and Baranchuk, Adrian},
