	author = {Adams, George L and Campbell, Paul T and Adams, John M and Strauss, David G and Wall, Karen and Patterson, Janet and Shuping, Kathy B and Maynard, Charles and Young, Dwayne and Corey, Craig and Thompson, Alan and Lee, Benjamin A and Wagner, Galen S},
