	pmid = {26683472},
	keywords = {Out-of-hospital cardiac arrest, Female, Humans, Male, Middle Aged, Retrospective Studies, Aged, Registries, Cardiopulmonary Resuscitation, Out-of-Hospital Cardiac Arrest, Propensity Score, Survival Rate, Cohort Studies, Survival, Extracorporeal Membrane Oxygenation, Extracorporeal circulation, Republic of Korea, Patient Discharge, Propensity score},
	pages = {26--32},
	file = {choi_2016_extracorporeal_life_support_and_survival_after_out-of-hospital_cardiac_arrest.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Resuscitation/2016/choi_2016_extracorporeal_life_support_and_survival_after_out-of-hospital_cardiac_arrest.pdf:application/pdf},
