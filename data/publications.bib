	note = {Publisher: BMJ Publishing Group Ltd and the British Association for Accident \& Emergency Medicine
Section: Commentary},
	keywords = {prehospital care, guidelines, communications, emergency department operations, errors},
	pages = {530--531},
	file = {hearns_2018_checklists_in_emergency_medicine.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Emergency Medicine Journal/2018/hearns_2018_checklists_in_emergency_medicine.pdf:application/pdf},
