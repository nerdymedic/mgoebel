	file = {ben-assuli_2019_data_mining_techniques_utilizing_latent_class_models_to_evaluate_emergency.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Journal of Biomedical Informatics/2019/ben-assuli_2019_data_mining_techniques_utilizing_latent_class_models_to_evaluate_emergency.pdf:application/pdf},
