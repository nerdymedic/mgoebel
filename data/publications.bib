	keywords = {Extracorporeal cardiopulmonary resuscitation, Out-of-hospital cardiac arrest, Prediction model},
	pages = {109652},
	file = {makino_2023_external_validation_of_the_tips65_score_for_predicting_good_neurological.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Resuscitation/2023/makino_2023_external_validation_of_the_tips65_score_for_predicting_good_neurological.pdf:application/pdf},
