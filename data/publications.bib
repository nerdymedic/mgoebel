	urldate = {2015-08-11},
	journal = {Circulation},
	author = {Lindau, Stacy Tessler and Abramsohn, Emily M and Bueno, Héctor and D'Onofrio, Gail and Lichtman, Judith H and Lorenze, Nancy P and Mehta Sanghani, Rupa and Spatz, Erica S and Spertus, John A and Strait, Kelly and Wroblewski, Kristen and Zhou, Shengfan and Krumholz, Harlan M},
	month = dec,
