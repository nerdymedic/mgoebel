	pmid = {29708085},
	keywords = {electrocardiography, Emergency Medical Services, STEMI, Electrocardiography, Female, Humans, Male, Middle Aged, Retrospective Studies, Aged, Myocardial Infarction, Time Factors, Los Angeles, Quality Improvement, EMS Emergency Medical Services, D2B door-to-balloon, ECG electrocardiogram, ECG transmission, FMC2B first-medical-contact-to-balloon, LA Los Angeles, MICN mobile intensive care nurse, PCI percutaneous coronary intervention, SRC STEMI Receiving Center, ST Elevation Myocardial Infarction, STEMI ST-elevation myocardial infarction, UCLA University of California},
	pages = {245--249},
