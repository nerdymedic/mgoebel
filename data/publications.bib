	keywords = {extracorporeal membrane oxygenation, Humans, Cardiopulmonary Resuscitation, Extracorporeal Membrane Oxygenation, Heart Arrest, ECMO, Bacteria, Bacterial Infections, ECMO CPR, Oxygenators, Membrane, primed ECMO circuits, safety},
	pages = {47--49},
	file = {weinberg_2015_is_it_safe_to_leave_an_ecmo_circuit_primed.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Perfusion/2015/weinberg_2015_is_it_safe_to_leave_an_ecmo_circuit_primed.pdf:application/pdf},
