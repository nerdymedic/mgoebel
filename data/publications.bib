	urldate = {2016-01-03},
	journal = {Annals of emergency medicine},
	author = {Mateos, Alonso and García-Lunar, Inés and García-Ruiz, José M and Pizarro, Gonzalo and Fernández-Jiménez, Rodrigo and Huertas, Pilar and García-Álvarez, Ana and Fernández-Friera, Leticia and Bravo, Jesús and Flores-Arias, José and Barreiro, María V and Chayán-Zas, Luisa and Corral, Ervigio and Fuster, Valentín and Sánchez-Brunete, Vicente and Ibáñez, Borja},
	month = mar,
