@article{schmidt_ease_2016,
	title = {Ease and difficulty of pre-hospital airway management in 425 paediatric patients treated by a helicopter emergency medical service: a retrospective analysis.},
	volume = {24},
	issn = {1757-7241},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26944389},
	doi = {10.1186/s13049-016-0212-9},
	abstract = {BACKGROUND Pre-hospital paediatric airway management is complex. A variety of pitfalls need prompt response to establish and maintain adequate ventilation and oxygenation. Anatomical disparity render laryngoscopy different compared to the adult. The correct choice of endotracheal tube size and depth of insertion is not trivial and often challenged due to the initially unknown age of child. METHODS Data from 425 paediatric patients ({\textless}17 years of age) with any airway manipulation treated by a Swiss Air-Ambulance crew between June 2010 and December 2013 were retrospectively analysed. Endpoints were: 1) Endotracheal intubation success rate and incidence of difficult airway management in primary missions. 2) Correlation of endotracheal tube size and depth of insertion with patient's age in all (primary and secondary) missions. RESULTS In primary missions, the first laryngoscopy-guided endotracheal intubation attempt was successful in 95.3\% of cases, with an overall success rate of 98.6\%. Difficult airway management was reported in 10 (4.7\%) patients. Endotracheal tube size was frequently chosen inadequately large (overall 50 of 343 patients: 14.6\%), especially and statistically significant in the age group below 1 year (19 of 33 patients; p {\textless} 0.001). Tubes were frequently and distinctively more deeply inserted (38.9\%) than recommended by current formulae. CONCLUSION Difficult airway management, including cannot intubate and cannot ventilate situations during pre-hospital paediatric emergency treatment was rare. In contrast, the success rate of endotracheal intubation at the first attempt was very high. High numbers of inadequate endotracheal tube size and deep placement according to patient age require further analysis. Practical algorithms need to be found to prevent potentially harmful treatment.},
	urldate = {2018-01-23},
	journal = {Scandinavian journal of trauma, resuscitation and emergency medicine},
	author = {Schmidt, Alexander R and Ulrich, Lea and Seifert, Burkhardt and Albrecht, Roland and Spahn, Donat R and Stein, Philipp},
	month = mar,
	year = {2016},
	pmid = {26944389},
	note = {Publisher: BioMed Central},
	pages = {22},
	file = {schmidt_2016_ease_and_difficulty_of_pre-hospital_airway_management_in_425_paediatric.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Scandinavian journal of trauma, resuscitation and emergency medicine/2016/schmidt_2016_ease_and_difficulty_of_pre-hospital_airway_management_in_425_paediatric.pdf:application/pdf},
}

