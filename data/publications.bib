@article{granfeldt_targeted_2021,
	title = {Targeted temperature management in adult cardiac arrest: {Systematic} review and meta-analysis},
	volume = {167},
	issn = {03009572},
	shorttitle = {Targeted temperature management in adult cardiac arrest},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0300957221003415},
	doi = {10.1016/j.resuscitation.2021.08.040},
	abstract = {Methods: PubMed, Embase, and the Cochrane Central Register of Controlled Trials were searched on June 17, 2021 for clinical trials. The population included adult patients with cardiac arrest. The review included all aspects of targeted temperature management including timing, temperature, duration, method of induction and maintenance, and rewarming. Two investigators reviewed trials for relevance, extracted data, and assessed risk of bias. Data were pooled using random-effects models. Certainty of evidence was evaluated using GRADE.
Results: The systematic search identiﬁed 32 trials. Risk of bias was assessed as intermediate for most of the outcomes. For targeted temperature management with a target of 32–34 °C vs. normothermia (which often required active cooling), 9 trials were identiﬁed, with six trials included in metaanalyses. Targeted temperature management with a target of 32–34 °C did not result in an improvement in survival (risk ratio: 1.08 [95\%CI: 0.89, 1.30]) or favorable neurologic outcome (risk ratio: 1.21 [95\%CI: 0.91, 1.61]) at 90 to 180 days after the cardiac arrest (low certainty of evidence). Three trials assessed different hypothermic temperature targets and found no difference in outcomes (low certainty of evidence). Ten trials were identiﬁed comparing prehospital cooling vs. no prehospital cooling with no improvement in survival (risk ratio: 1.01 [95\%CI: 0.92, 1.11]) or favorable neurologic outcome (risk ratio: 1.00 [95\%CI: 0.90, 1.11]) at hospital discharge (moderate certainty of evidence).
Conclusions: Among adult patients with cardiac arrest, the use of targeted temperature management at 32–34 °C, when compared to normothermia, did not result in improved outcomes in this meta-analysis. There was no effect of initiating targeted temperature management prior to hospital arrival. These ﬁndings warrant an update of international cardiac arrest guidelines.},
	language = {en},
	urldate = {2022-12-30},
	journal = {Resuscitation},
	author = {Granfeldt, Asger and Holmberg, Mathias J. and Nolan, Jerry P. and Soar, Jasmeet and Andersen, Lars W.},
	month = oct,
	year = {2021},
	pages = {160--172},
	file = {granfeldt_2021_targeted_temperature_management_in_adult_cardiac_arrest_-_systematic_review_and.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Resuscitation/2021/granfeldt_2021_targeted_temperature_management_in_adult_cardiac_arrest_-_systematic_review_and.pdf:application/pdf},
}

