@article{kern_outcomes_2015,
	title = {Outcomes of {Comatose} {Cardiac} {Arrest} {Survivors} {With} and {Without} {STEMI}},
	volume = {8},
	issn = {19368798},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/26117462},
	doi = {10.1016/j.jcin.2015.02.021},
	abstract = {OBJECTIVES{\textbackslash}nThe aim of this study was to compare outcomes and coronary angiographic findings in post–cardiac arrest patients with and without ST-segment elevation myocardial infarction (STEMI). {\textbackslash}n{\textbackslash}nBACKGROUND{\textbackslash}nThe 2013 STEMI guidelines recommend performing immediate angiography in resuscitated patients whose initial electrocardiogram shows STEMI. The optimal approach for those without STEMI post–cardiac arrest is less clear. {\textbackslash}n{\textbackslash}nMETHODS{\textbackslash}nA retrospective evaluation of a post–cardiac arrest registry was performed. {\textbackslash}n{\textbackslash}nRESULTS{\textbackslash}nThe database consisted of 746 comatose post–cardiac arrest patients including 198 with STEMI (26.5\%) and 548 without STEMI (73.5\%). Overall survival was greater in those with STEMI compared with those without (55.1\% vs. 41.3\%; p = 0.001), whereas in all patients who underwent immediate coronary angiography, survival was similar between those with and without STEMI (54.7\% vs. 57.9\%; p = 0.60). A culprit vessel was more frequently identified in those with STEMI, but also in one-third of patients without STEMI (80.2\% vs. 33.2\%; p = 0.001). The majority of culprit vessels were occluded (STEMI, 92.7\%; no STEMI, 69.2\%; p {\textless} 0.0001). An occluded culprit vessel was found in 74.3\% of STEMI patients and in 22.9\% of no STEMI patients. Among cardiac arrest survivors discharged from the hospital who had presented without STEMI, coronary angiography was associated with better functional outcome (93.3\% vs. 78.7\%; p {\textless} 0.003). {\textbackslash}n{\textbackslash}nCONCLUSIONS{\textbackslash}nEarly coronary angiography is associated with improved functional outcome among resuscitated patients with and without STEMI. Resuscitated patients with a presumed cardiac etiology appear to benefit from immediate coronary angiography.},
	number = {8},
	urldate = {2016-01-03},
	journal = {JACC: Cardiovascular Interventions},
	author = {Kern, Karl B. and Lotun, Kapildeo and Patel, Nainesh and Mooney, Michael R. and Hollenbeck, Ryan D. and McPherson, John A. and McMullan, Paul W. and Unger, Barbara and Hsu, Chiu-Hsieh and Seder, David B.},
	month = jul,
	year = {2015},
	pmid = {26117462},
	note = {Publisher: Elsevier Inc},
	keywords = {cardiac arrest, percutaneous coronary intervention, STEMI, electrocardiogram, ST-segment elevation myocardial infarction, ECG, CAG, cerebral performance category, chronic total occlusion, coronary angiography, coronary angiography, CPC, CTO, hypothermia, LAD, LCX, left anterior descending artery, left circumflex artery, PCI, post-resuscitation syndrome, RCA, right coronary artery, targeted temperature management, TTM},
