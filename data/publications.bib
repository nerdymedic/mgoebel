	keywords = {Cardiac arrest, Cardiopulmonary resuscitation, Emergency Medical Services, Adult, Humans, Cardiopulmonary Resuscitation, Out-of-Hospital Cardiac Arrest, England, Epinephrine, Administration, Intravenous, Intraosseous, Intravenous, Adrenaline, State Medicine, Wales},
	pages = {954--962},
