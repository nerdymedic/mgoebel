	pmid = {18178399},
	note = {ISBN: 0002-9149},
	keywords = {Emergency Medical Services, Electrocardiography, Female, Humans, Male, Middle Aged, Myocardial Infarction, Time Factors, Myocardial Infarction: therapy, Prospective Studies, Emergency Medical Services: standards, Emergency Service, Hospital, Myocardial Infarction: diagnosis, Angioplasty, Balloon, Coronary, Triage, Transportation of Patients, Hospitals, California, Electrocardiography: utilization, Hospital: standards, Time and Motion Studies, Transportation of Patients: standards, Urban},
