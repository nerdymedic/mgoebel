@article{witting_evaluation_2020,
	title = {Evaluation of {Spodick}'s {Sign} and {Other} {Electrocardiographic} {Findings} as {Indicators} of {STEMI} and {Pericarditis}},
	volume = {58},
	issn = {0736-4679},
	doi = {10.1016/j.jemermed.2020.01.017},
	abstract = {BACKGROUND: Patients with ST elevation on electrocardiogram (ECG) could have ST elevation myocardial infarction (STEMI) or pericarditis. Spodick's sign, a downsloping of the ECG baseline (the T-P segment), has been described, but not validated, as a sign of pericarditis.
OBJECTIVE: This study estimates the frequency of Spodick's sign and other findings in patients diagnosed with STEMI and those with pericarditis.
METHODS: In this retrospective review, we selected charts that met prospective definitions of STEMI (cases) and pericarditis (controls). We excluded patients whose ECGs lacked ST elevation. An authority on electrocardiography reviewed all ECGs, noting the presence or absence of Spodick's sign, ST depression (in leads besides V1 and aVR), PR depression, greater ST elevation in lead III than in lead II (III {\textgreater} II), abrupt take-off of ST segment (the RT checkmark sign), and upward or horizontal ST convexity. We quantified strength of association using odds ratio (OR) with 95\% confidence interval (CI).
RESULTS: One hundred and sixty-five patients met criteria for STEMI and 42 met those for pericarditis. Spodick's sign occurred in 5\% of patients with STEMI (95\% CI 3-10\%) and 29\% of patients with pericarditis (95\% CI 16-45\%). All other findings statistically distinguished STEMI from pericarditis, but ST depression (OR 31), III {\textgreater} II (OR 21), and absence of PR depression (OR 12) had the greatest OR values.
CONCLUSIONS: Spodick's sign is statistically associated with pericarditis, but it is seen in 5\% of patients with STEMI. Among other findings, ST depression, III {\textgreater} II, and absence of PR depression were the most discriminating.},
	language = {eng},
	number = {4},
	journal = {The Journal of Emergency Medicine},
	author = {Witting, Michael D. and Hu, Kami M. and Westreich, Aaron A. and Tewelde, Semhar and Farzad, Ali and Mattu, Amal},
	month = apr,
	year = {2020},
	pmid = {32222321},
	keywords = {chest pain, electrocardiography, emergency service, hospital, pericarditis, ST elevation myocardial infarction},
	pages = {562--569},
	file = {witting_2020_evaluation_of_spodick's_sign_and_other_electrocardiographic_findings_as.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/The Journal of Emergency Medicine/2020/witting_2020_evaluation_of_spodick's_sign_and_other_electrocardiographic_findings_as.pdf:application/pdf},
