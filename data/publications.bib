	year = {1997},
	keywords = {Female, Humans, Male, Aged, Aged, 80 and over, Risk Factors, Homes for the Aged, Nursing Homes, Multivariate Analysis, Blood Pressure, Analysis of Variance, Blood Pressure Determination, Frail Elderly, Hypotension, Orthostatic, Prevalence},
