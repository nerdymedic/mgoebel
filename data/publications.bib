@article{kerem_role_2014,
	title = {The role of prehospital electrocardiograms in the recognition of {ST}-segment elevation myocardial infarctions and reperfusion times.},
	volume = {46},
	issn = {0736-4679},
	url = {http://www.sciencedirect.com/science/article/pii/S0736467913010755},
	doi = {10.1016/j.jemermed.2013.08.084},
	abstract = {BACKGROUND: Clinical outcomes in ST-segment elevation myocardial infarction (STEMI) are related to reperfusion times. Given the benefit of early recognition of STEMI and resulting ability to decrease reperfusion times and improve mortality, current prehospital recommendations are to obtain electrocardiograms (ECGs) in patients with concern for acute coronary syndrome.

OBJECTIVES: We sought to determine the effect of wireless transmission of prehospital ECGs on STEMI recognition and reperfusion times. We hypothesized decreased reperfusion times in patients in whom prehospital ECGs were obtained.

METHODS: We conducted a retrospective, observational study of patients who presented to our suburban, tertiary care, teaching hospital emergency department with STEMI on a prehospital ECG.

RESULTS: Ninety-nine patients underwent reperfusion therapy. Patients with prehospital ECGs had a mean time to angioplasty suite of 43 min (95\% confidence interval [CI] 31-54). Compared to patients with no prehospital ECG, mean time to angioplasty suite was 49 min (95\% CI 41-57), p = 0.035. Patients with prehospital STEMI identification and catheterization laboratory activation had a mean time to angioplasty suite of 33 min (95\% CI 25-41), p = 0.007. Patients with prehospital ECGs had a mean door-to-balloon time of 66 min (95\% CI 53-79), whereas the control group had a mean door-to-balloon time of 79 min (95\% CI 67-90), p = 0.024. Patients with prehospital STEMI identification and catheterization laboratory activation had a mean door-to-balloon time of 58 min (95\% CI 48-68), p = 0.018.

CONCLUSIONS: Prehospital STEMI identification allows for prompt catheterization laboratory activation, leading to decreased reperfusion times.},
	number = {2},
	urldate = {2015-08-17},
	journal = {The Journal of emergency medicine},
	author = {Kerem, Yaniv and Eastvold, Joshua S and Faragoi, DeAnn and Strasburger, Diana and Motzny, Sean E and Kulstad, Erik B},
	month = feb,
	year = {2014},
	pmid = {24268634},
	keywords = {Emergency Medical Services, Adult, Electrocardiography, Female, Humans, Male, Retrospective Studies, Case-Control Studies, Myocardial Infarction, Emergency Medical Services: methods, Myocardial Infarction: therapy, Time-to-Treatment, Emergency Service, Hospital, Myocardial Infarction: diagnosis, Angioplasty, Balloon, Coronary, Coronary: statistics \& numer, Cardiac Catheterization, Hospital: organization \& admini, Cardiac Catheterization: statistics \& numerical da, Myocardial Reperfusion Injury, Myocardial Reperfusion Injury: diagnosis, Time-to-Treatment: statistics \& numerical data},
	pages = {202--7},
	file = {kerem_2014_the_role_of_prehospital_electrocardiograms_in_the_recognition_of_st-segment.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/The Journal of emergency medicine/2014/kerem_2014_the_role_of_prehospital_electrocardiograms_in_the_recognition_of_st-segment.pdf:application/pdf},
