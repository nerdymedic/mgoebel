@article{semler_balanced_2018,
	title = {Balanced {Crystalloids} versus {Saline} in {Critically} {Ill} {Adults}},
	volume = {378},
	issn = {0028-4793},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5846085/},
	doi = {10.1056/NEJMoa1711584},
	abstract = {BACKGROUND
Both balanced crystalloids and saline are used for intravenous fluid administration in critically ill adults, but it is not known which results in better clinical outcomes.

METHODS
In a pragmatic, cluster-randomized, multiple-crossover trial conducted in five intensive care units at an academic center, we assigned 15,802 adults to receive saline (0.9\% sodium chloride) or balanced crystalloids (lactated Ringer’s solution or Plasma-Lyte A) according to the randomization of the unit to which they were admitted. The primary outcome was a major adverse kidney event within 30 days — a composite of death from any cause, new renal-replacement therapy, or persistent renal dysfunction (defined as an elevation of the creatinine level to ≥200\% of baseline) — all censored at hospital discharge or 30 days, whichever occurred first.

RESULTS
Among the 7942 patients in the balanced-crystalloids group, 1139 (14.3\%) had a major adverse kidney event, as compared with 1211 of 7860 patients (15.4\%) in the saline group (marginal odds ratio, 0.91; 95\% confidence interval [CI], 0.84 to 0.99; conditional odds ratio, 0.90; 95\% CI, 0.82 to 0.99; P = 0.04). In-hospital mortality at 30 days was 10.3\% in the balanced-crystalloids group and 11.1\% in the saline group (P = 0.06). The incidence of new renal-replacement therapy was 2.5\% and 2.9\%, respectively (P = 0.08), and the incidence of persistent renal dysfunction was 6.4\% and 6.6\%, respectively (P = 0.60).

CONCLUSIONS
Among critically ill adults, the use of balanced crystalloids for intravenous fluid administration resulted in a lower rate of the composite outcome of death from any cause, new renal-replacement therapy, or persistent renal dysfunction than the use of saline. (Funded by the Vanderbilt Institute for Clinical and Translational Research and others; SMART-MED and SMART-SURG ClinicalTrials.gov numbers, NCT02444988 and NCT02547779.)},
	number = {9},
	urldate = {2019-08-08},
	journal = {The New England journal of medicine},
	author = {Semler, Matthew W. and Self, Wesley H. and Wanderer, Jonathan P. and Ehrenfeld, Jesse M. and Wang, Li and Byrne, Daniel W. and Stollings, Joanna L. and Kumar, Avinash B. and Hughes, Christopher G. and Hernandez, Antonio and Guillamondegui, Oscar D. and May, Addison K. and Weavind, Liza and Casey, Jonathan D. and Siew, Edward D. and Shaw, Andrew D. and Bernard, Gordon R. and Rice, Todd W.},
	month = mar,
	year = {2018},
	pmid = {29485925},
	pmcid = {PMC5846085},
	pages = {829--839},
	file = {semler_2018_balanced_crystalloids_versus_saline_in_critically_ill_adults.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/The New England journal of medicine/2018/semler_2018_balanced_crystalloids_versus_saline_in_critically_ill_adults.pdf:application/pdf},
}

@article{al-akchar_mohammad_reliance_2017,
	title = {Reliance on {Electrocardiogram} {Computer} {Algorithm} {Interpretation} to {Activate} {ST} {Elevation} {Myocardial} {Infarction} {Processes} of {Care} and {Initiate} {Reperfusion} {Therapy}: {Impact} on {False} {Activation}},
	volume = {136},
	shorttitle = {Abstract 18253},
	url = {https://www.ahajournals.org/doi/abs/10.1161/circ.136.suppl_1.18253},
	doi = {10.1161/circ.136.suppl_1.18253},
	abstract = {Background: Rapid clinical assessment and accurate ECG interpretation are requisites to ensure expeditious initiation of reperfusion therapy in patients (pts) with presumed ST elevation myocardial infarction (STEMI). Efforts to facilitate triage include reliance on computer algorithm ECG analyses, especially in rural U.S. communities and low-volume Emergency Departments.Methods: In this prospective, observational study, we analyzed the “activating” ECG (AECG) of 4,123 consecutive pts with presumed STEMI, managed within a regional STEMI reperfusion program in rural Central and Southern IL between 1/2005-12/2014. AECG’s were categorized based on the computer interpretation as “ischemic” (Isch-ECG) in the presence of the following phrases: “acute MI/injury”, “ST elevation” and “acute ischemia”. STEMI-confirmed adjudication (STEMI-C) was defined as: a.)AECG with ST elevation/LBBB , plus b.) totally occluded infarct artery or {\textgreater}60\% stenosis with filling defect on coronary angiogram, plus c.) elevated biomarker. Absence of criteria defined False activation (FA) pts.Results: Computer analyzable AECG’s were available in 3,584 (87\%) pts ; 2,685 (75\%) adjudicated as STEMI-C and 899 (25\%) as FA. An Isch-ECG was noted in 481 (54\%) and 2,351 (88\%) of FA and STEMI-C pts respectively (p{\textless}0.01). Co-morbidities were more frequently observed among the non-Isch-ECG pts, while more expeditious STEMI care was provided to the Isch-ECG group. Fibrinolytic was administered to 2.4\% of FA pts. Sensitivity and specificity of ECG computer analysis for STEMI confirmation was 87.5\% (95\% CI: 86\%-89\%) and 46.5\% (95\% CI: 43\%-50\%), respectively.Download figureDownload PowerPointConclusions: In this rural, STEMI care program, an AECG with computer interpretation of “ischemia” was associated with shorter DTB and ED dwell times, but high FA rate; observed in 54\% of all FA pts. Potential benefits and risks of STEMI treatment and triage based on ECG computer analysis must be carefully weighted within each STEMI system.},
	number = {suppl\_1},
	urldate = {2019-07-30},
	journal = {Circulation},
	author = {{Al-akchar Mohammad} and {Aguirre Frank V} and {Mahmaljy Hadi} and {Venturini Joseph} and {Besser Stephanie} and {Chambers Jenny} and {Goel Sachin} and {Panchamukhi Varadendra} and {Kalathiya Rohan} and {Al-Dallow Raed} and {Shah Atman} and {Liao James K}},
	month = nov,
	year = {2017},
	pages = {A18253--A18253},
}

