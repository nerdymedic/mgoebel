@article{bradley_achieving_2006,
	title = {Achieving {Rapid} {Door}-{To}-{Balloon} {Times}: {How} {Top} {Hospitals} {Improve} {Complex} {Clinical} {Systems}},
	volume = {113},
	issn = {0009-7322},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/16490818},
	doi = {10.1161/CIRCULATIONAHA.105.590133},
	abstract = {BACKGROUND: Fewer than half of patients with ST-elevation acute myocardial infarction (STEMI) are treated within guideline-recommended door-to-balloon times; however, little information is available about the approaches used by hospitals that have been successful in improving door-to-balloon times to meet guidelines. We sought to characterize experiences of hospitals with outstanding improvement in door-to-balloon time during 1999-2002.

METHODS AND RESULTS: We performed a qualitative study using in-depth interviews (n=122) with clinical and administrative staff at 11 hospitals that were participating with the National Registry of Myocardial Infarction and had median door-to-balloon times of {\textless} or =90 minutes during 2001-2002, representing substantial improvement since 1999. Data were organized with the use of NUD-IST 4 (Sage Publications Software) and were analyzed by the constant comparative method of qualitative data analysis. Eight themes characterized hospitals' experiences: commitment to an explicit goal to improve door-to-balloon time motivated by internal and external pressures; senior management support; innovative protocols; flexibility in refining standardized protocols; uncompromising individual clinical leaders; collaborative teams; data feedback to monitor progress and identify problems and successes; and an organizational culture that fostered resilience to challenges or setbacks in improvement efforts.

CONCLUSIONS: Several themes characterized the experiences of hospitals that had achieved notable improvements in their door-to-balloon times. By distilling the complex and diverse experiences of organizational change into its essential components, this study provides a foundation for future efforts to elevate clinical performance in the hospital setting.},
	number = {8},
	urldate = {2015-09-28},
	journal = {Circulation},
	author = {Bradley, Elizabeth H. and Curry, Leslie A and Webster, Tashonna R and Mattera, Jennifer A and Roumanis, Sarah A and Radford, Martha J and McNamara, Robert L and Barton, Barbara A and Berg, David N and Krumholz, Harlan M},
	month = feb,
	year = {2006},
	pmid = {16490818},
	keywords = {Humans, Myocardial Infarction, Time Factors, Myocardial Infarction: therapy, Angioplasty, Balloon, Coronary, Coronary: standards, Myocardial Infarction: mortality, Quality of Health Care, Hospitals, cardial infarction, ecent evidence from the, elevation myocardial, Guideline Adherence, half of patients with, Health Services, health services research ⅲ, Health Services: standards, Hospitals: standards, indicates that fewer than, Interviews as Topic, myocardial infarction ⅲ quality, national registry of myo-, nrmi, Quality of Health Care: statistics \& numerical dat, st-segment},
