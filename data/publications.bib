	keywords = {ECPR, Outcome, Adult, Female, Humans, Male, Middle Aged, Retrospective Studies, Aged, Cardiopulmonary Resuscitation, Survival Rate, Arrhythmias, Cardiac, Electric Countershock, Survival, Extracorporeal Membrane Oxygenation, Heart Arrest, ECMO, Databases, Factual, Kaplan-Meier Estimate, PEA},
