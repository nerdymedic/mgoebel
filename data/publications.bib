@article{ehrlich_endotracheal_2004,
	title = {Endotracheal intubations in rural pediatric trauma patients},
	volume = {39},
	issn = {0022-3468, 1531-5037},
	url = {https://www.jpedsurg.org/article/S0022-3468(04)00349-5/abstract},
	doi = {10.1016/j.jpedsurg.2004.05.010},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Background/purpose{\textless}/h3{\textgreater}{\textless}p{\textgreater}Evidence from urban trauma centers questions the efficacy of pediatric field endotracheal intubations (ETIs). It is recognized that in the rural environment, discovery, transport delays, and a paucity of pediatric expertise contribute to higher pediatric trauma mortality rates compared with urban environments. The purpose of our study was to determine the effectiveness of field ETI in rural pediatric trauma patients.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}ETI attempts (field, referring hospital, trauma center [TC]) in trauma patients less than 19 years old were included. Prehospital and TC charts, including demographics, injury mechanism, indication, location, person performing, number of attempts, Glascow Coma Scale (GCS), complications from ETI, and outcome, were assessed.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Between 1991 and 2000, 105 of 2,907 patients met study criteria. Paramedics, trauma flight nurses (field ETIs), emergency physicians, surgeons, and anesthesiologists performed the ETI. One hundred fiftyfive ETIs (1 to 6 per patient) were attempted in 105 children. Fifty-seven percent of the ETIs were attempted in the field, 22\% in transferring hospital, and 21\% at the TC. Successful intubation on first attempt was 67\% (field), 69\% (referring hospital), and 95\% (TC). Subsequent ETI attempts had failure rates of 50\% (field) and 0\% (referring hospital, TC). Indication for ETI included fear of losing airway control (37\%), closed head injury (36.1\%), respiratory rate less than 10 or greater than 40 (11.2\%), cardiopulmonary arrest (6.5\%), respiratory arrest (4.6\%), and airway obstruction 4.6\%. Only 9.3\% of children could not be oxygenated or ventilated by bag valve mask (BVM) before ETI. Twenty-three percent had complications directly related to ETI (eg, aspiration). The relative risk of an airway complication was 2.5× higher with more then one ETI attempt (\textit{P} {\textless} .05). Four percent of the airway complications occurred in TC, 29\% (transferring hospital) and 66\% (field, \textit{P} {\textless} .05), respectively. Airway complications and multiple ETIs were associated with transport delay, lower GCS, longer hospital stay, and lower discharge GCS (\textit{P} {\textless} .001) but independent of injury severity score, sex, age, and survival (\textit{P} {\textgreater} .05).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions{\textless}/h3{\textgreater}{\textless}p{\textgreater}Multiple ETI attempts are associated with significant complications and may offer limited advantage over BVM and possibly may affect outcome. Indications for field intubations may require review especially in rural pediatric trauma.{\textless}/p{\textgreater}},
	language = {English},
	number = {9},
	urldate = {2019-09-07},
	journal = {Journal of Pediatric Surgery},
	author = {Ehrlich, P. F. and Seidman, P. S. and Atallah, O. and Haque, A. and Helmkamp, J.},
	month = sep,
	year = {2004},
	pmid = {15359393},
	pages = {1376--1380},
	file = {ehrlich_2004_endotracheal_intubations_in_rural_pediatric_trauma_patients.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Journal of Pediatric Surgery/2004/ehrlich_2004_endotracheal_intubations_in_rural_pediatric_trauma_patients.pdf:application/pdf},
}

@article{dunford_incidence_2003,
	title = {Incidence of transient hypoxia and pulse rate reactivity during paramedic rapid sequence intubation},
	volume = {42},
	issn = {0196-0644, 1097-6760},
	url = {https://www.annemergmed.com/article/S0196-0644(03)00660-7/abstract},
	doi = {10.1016/S0196-0644(03)00660-7},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Study objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}We determine the incidence of desaturation and pulse rate reactivity during paramedic rapid sequence intubation of patients with severe head injuries (Glasgow Coma Scale score ≤8).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}Adult patients with severe head injuries had recording oximeter-capnometers applied before rapid sequence intubation. Desaturation was defined as a reduction in oxygen saturation (Spo$_{\textrm{2}}$) to less than 90\% from an initial Spo$_{\textrm{2}}$ of greater than or equal to 90\% or a decrease from a baseline of less than 90\%. Event records were analyzed with emergency medical services (EMS) run sheets and debriefing reports.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Thirty-one (57\%) of 54 patients demonstrated desaturation during rapid sequence intubation. Twenty-six (84\%) of these 31 events occurred in patients whose initial Spo$_{\textrm{2}}$ value with basic airway skills was greater than or equal to 90\%. The median duration of desaturation was 160 seconds (interquartile range 48 to 272 seconds), and the median decrease in Spo$_{\textrm{2}}$ was 22\%. Six (19\%) patients experienced marked bradycardia (pulse rate {\textless}50 beats/min) during desaturation events. Paramedics described rapid sequence intubation as "easy" in 26 (84\%) of 31 patients with desaturation.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusion{\textless}/h3{\textgreater}{\textless}p{\textgreater}Out-of-hospital rapid sequence intubation by paramedics was complicated by a concerning incidence of desaturation and bradycardia. Paramedic reports did not reflect the presence of these concerning derangements. Most patients had acceptable Spo$_{\textrm{2}}$ values before rapid sequence intubation. An effective strategy for preoxygenation is needed before it can be concluded that rapid sequence intubation is of value in the out-of-hospital care of patients with serious closed head injury.{\textless}/p{\textgreater}},
	language = {English},
	number = {6},
	urldate = {2019-09-07},
	journal = {Annals of Emergency Medicine},
	author = {Dunford, James V. and Davis, Daniel P. and Ochs, Mel and Doney, Michael and Hoyt, David B.},
	month = dec,
	year = {2003},
	pages = {721--728},
	file = {dunford_2003_incidence_of_transient_hypoxia_and_pulse_rate_reactivity_during_paramedic_rapid.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Annals of Emergency Medicine/2003/dunford_2003_incidence_of_transient_hypoxia_and_pulse_rate_reactivity_during_paramedic_rapid.pdf:application/pdf},
}

@article{vilke_out--hospital_2002,
	title = {Out-of-hospital pediatric intubation by paramedics: the {San} {Diego} experience},
	volume = {22},
	issn = {0736-4679},
	shorttitle = {Out-of-hospital pediatric intubation by paramedics},
	doi = {10.1016/s0736-4679(01)00439-5},
	abstract = {To evaluate pediatric endotracheal intubations by our paramedics, we performed a retrospective review of a prehospital computer database, quality assurance reviews, and prehospital run sheets for all patients under 15 years of age who had an endotracheal tube (ETT) placed. During the 4.5-year study period, 324 pediatric patients had intubation attempts by field paramedics, of which 264 (82\%) were successful and three were reported esophageal and unrecognized by the paramedic. Two of these esophageal placements were noted on arrival at the hospital, and one upon turn-over of patient care to a nurse of an aeromedical service. All three intubations were deemed esophageal with direct laryngoscopy, and the patients had been in cardiopulmonary arrest status prior to the intubation. Of the 264 patients who had ETT placed, 99\% were endotracheal, while only 1\% were unrecognized esophageal. We conclude that pediatric endotracheal intubation by out-of-hospital paramedics in an established EMS system has a low occurrence of unrecognized esophageal placements.},
	language = {eng},
	number = {1},
	journal = {The Journal of Emergency Medicine},
	author = {Vilke, Gary M. and Steen, Pamela J. and Smith, Alan M. and Chan, Theodore C.},
	month = jan,
	year = {2002},
	pmid = {11809559},
	keywords = {Emergency Medical Services, Humans, Intubation, Intratracheal, Emergency Medical Technicians, Child, California, Clinical Competence, Intubation, Esophagus, Quality Assurance, Health Care},
	pages = {71--74},
	file = {vilke_2002_out-of-hospital_pediatric_intubation_by_paramedics_-_the_san_diego_experience.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/The Journal of Emergency Medicine/2002/vilke_2002_out-of-hospital_pediatric_intubation_by_paramedics_-_the_san_diego_experience.pdf:application/pdf},
