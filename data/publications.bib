	number = {3},
	urldate = {2023-05-09},
	journal = {JACC: Cardiovascular Interventions},
	author = {Tonna, Joseph E. and Selzman, Craig H. and Girotra, Saket and Presson, Angela P. and Thiagarajan, Ravi R. and Becker, Lance B. and Zhang, Chong and Rycus, Peter and Keenan, Heather T.},
	month = feb,
	year = {2022},
	keywords = {extracorporeal membrane oxygenation, extracorporeal cardiopulmonary resuscitation, mortality prediction, in-hospital cardiac arrest, survival prediction},
	pages = {237--247},
	file = {tonna_2022_resuscitation_using_ecpr_during_in-hospital_cardiac_arrest_(rescue-ihca).pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/JACC Cardiovascular Interventions/2022/tonna_2022_resuscitation_using_ecpr_during_in-hospital_cardiac_arrest_(rescue-ihca).pdf:application/pdf},
