	keywords = {Adult, Female, Humans, Male, Middle Aged, Time Factors, Young Adult, Brain Injuries, Brain Injuries: therapy, Glasgow Coma Scale, Glasgow Coma Scale: statistics \& numerical data, Glasgow Outcome Scale, Glasgow Outcome Scale: statistics \& numerical data, Shock, Shock: mortality, Shock: therapy, Trauma Centers, Trauma Centers: statistics \& numerical data},
	pages = {30--41, 41.e1--3},
	file = {newgard_2015_revisiting_the_golden_hour_-_an_evaluation_of_out-of-hospital_time_in_shock.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Annals of emergency medicine/2015/newgard_2015_revisiting_the_golden_hour_-_an_evaluation_of_out-of-hospital_time_in_shock.pdf:application/pdf},
