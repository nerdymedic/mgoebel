	journal = {Journal of the American College of Cardiology},
	author = {Kudenchuk, Peter J. and Ho, Mary T. and Weaver, W.Douglas D and Litwin, Paul E. and Martin, Jenny S. and Eisenberg, Mickey S. and Hallstrom, Alfred P. and Cobb, Leonard A. and Kennedy, J.Ward W},
