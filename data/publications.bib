@article{ruetzler_performance_2011,
	title = {Performance and skill retention of intubation by paramedics using seven different airway devices—{A} manikin study},
	volume = {82},
	issn = {0300-9572, 1873-1570},
	url = {https://www.resuscitationjournal.com/article/S0300-9572(11)00050-5/abstract},
	doi = {10.1016/j.resuscitation.2011.01.008},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Introduction{\textless}/h3{\textgreater}{\textless}p{\textgreater}Endotracheal intubation (ETI) is the most widespread method for emergency airway management. Several studies reported that ETI requires considerable skill and experience and if performed incorrectly, may result in serious adverse events. Unrecognized tube misplacement or oesophageal intubation is associated with high prehospital morbidity. This study investigates the usability of supraglottic airway devices compared to ETI and the skill retention of 41 previously inexperienced paramedics following training using a manikin model.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}41 paramedics participated in this study. None had prior experience in airway management, apart from bag-valve ventilation. After a standardised audio-visual lecture lasting 45min, the paramedics participated in a practical demonstration using the advanced patient simulator SimMan$^{\textrm{®}}$ (Laerdal Medical, Stavanger, Norway). Afterwards, paramedics were instructed to perform airway-management using seven different techniques to secure the airway (ETI, Laryngeal mask unique [LMA], Proseal, Laryngeal tube disposable [LT-D$^{\textrm{®}}$], I-Gel$^{\textrm{®}}$, Combitube$^{\textrm{®}}$, and EasyTube$^{\textrm{®}}$) following a randomized sequence. Participants underwent reassessment after 3 months without any further training or practice in airway-management.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}During the initial training session, ETI was successfully performed in 78\% of cases, while 3 months later the success rate was 58\%. For the supraglottic airway devices, five out of six were successfully used by all paramedics at both time points, the exception being Proseal$^{\textrm{®}}$. Our data show successful skill retention (success rate: 100\%) after 3 months for five out of six supraglottic airway devices. Time to ventilation (T3) was significantly less for LMA, LT-D$^{\textrm{®}}$ and I-Gel$^{\textrm{®}}$ at all time points compared to ETI.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusion{\textless}/h3{\textgreater}{\textless}p{\textgreater}ETI performed by inexperienced paramedics is associated with a low success rate. In contrast, supraglottic airway devices like LMA, LT-D$^{\textrm{®}}$, I-Gel$^{\textrm{®}}$, Combitube$^{\textrm{®}}$ and EasyTube$^{\textrm{®}}$ are fast, safe and easy-to-use. Within the limitations of a manikin-study, this study suggests that inexperienced medical staff might benefit from using supraglottic airway devices for emergency airway management.{\textless}/p{\textgreater}},
	language = {English},
	number = {5},
	urldate = {2019-09-07},
	journal = {Resuscitation},
	author = {Ruetzler, Kurt and Roessler, Bernhard and Potura, Lukasz and Priemayr, Anita and Robak, Oliver and Schuster, Ernst and Frass, Michael},
	month = may,
	year = {2011},
	pages = {593--597},
	file = {ruetzler_2011_performance_and_skill_retention_of_intubation_by_paramedics_using_seven.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Resuscitation/2011/ruetzler_2011_performance_and_skill_retention_of_intubation_by_paramedics_using_seven.pdf:application/pdf},
}

