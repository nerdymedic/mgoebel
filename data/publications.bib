	keywords = {Emergency Medical Services, Electrocardiography, Humans, Predictive Value of Tests, Sensitivity and Specificity, Myocardial Infarction, Emergency Medical Services: methods, Myocardial Infarction: therapy, Algorithms, Emergency Service, Hospital, Myocardial Infarction: diagnosis, Angioplasty, Balloon, Coronary, Triage, Triage: methods, Myocardial Infarction: epidemiology, Outcome and Process Assessment (Health Care), Regional Health Planning, Los Angeles, Los Angeles: epidemiology, Hospitals, Bayes Theorem, Community, Community: statistics \& numerical data, Hospital: statistics \& numerica, Myocardial Revascularization, Patient Transfer},
	pages = {1165--71},
