@article{johnson_dehydration_1995,
	title = {Dehydration and {Orthostatic} {Vital} {Signs} in {Women} with {Hyperemesis} {Gravidarum}},
	volume = {2},
	copyright = {© 1995 Society for Academic Emergency Medicine},
	issn = {1553-2712},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1553-2712.1995.tb03620.x},
	doi = {10.1111/j.1553-2712.1995.tb03620.x},
	abstract = {Objectives: To assess the hydration status of women presenting to an ED with hyperemesis gravidarum and to determine whether clinically relevant changes in orthostatic vital signs occur. Methods: A convenience sample of 23 pregnant women who had hyperemesis gravidarum, with each patient serving as her own control. The study took place in the ED observation unit of an urban teaching hospital. Women who had pregnancies of ≤16 weeks' gestation who had been vomiting for at least 24 hours were included. Supine and standing pulse rates and blood pressures (BPs) were measured sequentially after 5 minutes in each position. Patient weight and urine specific gravity (SG) also were recorded. After 6 L of lactated Ringer's solution was infused over a 12-hour period, the same measurements were repeated. Pre- and posthydration changes were analyzed using the paired t-test. Results: The mean treatment weight gain as a percentage of the total body weight was 5.6\% ± 2.2\% (mean ± SD). The urine SG decreased from 1.027 ± 0.004 to 1.008 ± 0.003 (p {\textless} 0.001). The mean change in systolic BP upon assuming the standing position was -8.3 ± 12.7 mm Hg before hydration vs 2.9 ± 7.8 mm Hg after hydration (p {\textless} 0.001). The corresponding change in mean diastolic BP was 3.7 ± 10.9 mm Hg before hydration vs 8.6 ± 10.9 mm Hg after hydration (p = 0.12). The mean change in pulse rate upon standing was 26.8 ± 14.5 beats/min before hydration vs 14.5 ± 10.1 beats/min after hydration (p = 0.002). Conclusions: Women who present to the ED with hyperemesis gravidarum are significantly dehydrated and experience measurable improvement in postural pulse rate and systolic BP changes with rehydration. However, the presenting orthostatic changes lack sufficient sensitivity to be effectively used as quantitative screening tests for dehydration.},
	language = {en},
	number = {8},
	urldate = {2019-01-29},
	journal = {Academic Emergency Medicine},
	author = {Johnson, David R. and Douglas, Daniel and Hauswald, Mark and Tandberg, Dan},
	year = {1995},
	keywords = {blood pressure, hyperemesis gravidarum, orthostatic vital signs, pregnancy, receiver operating characteristic curves, tilt test},
	pages = {692--697},
	file = {johnson_1995_dehydration_and_orthostatic_vital_signs_in_women_with_hyperemesis_gravidarum.pdf:/Users/mat/Library/CloudStorage/GoogleDrive-mgoebel@gmail.com/My Drive/Zotero_Library/Academic Emergency Medicine/1995/johnson_1995_dehydration_and_orthostatic_vital_signs_in_women_with_hyperemesis_gravidarum.pdf:application/pdf},
}

@article{aronow_prevalence_1988,
	title = {Prevalence of postural hypotension in elderly patients in a long-term health care facility},
	volume = {62},
	issn = {0002-9149},
	url = {http://www.sciencedirect.com/science/article/pii/0002914988902433},
	doi = {10.1016/0002-9149(88)90243-3},
	abstract = {Many etiologic factors, often coexisting, can produce postural hypotension in elderly patients including impaired baroreceptor reflex sensitivity, loss of arterial compliance, reduction of venous return due to increased venous distensibility, impairment of compensatory mechanisms for maintenance of fluid volume and electrolyte balance, an increased incidence of common precipitating diseases and disorders and the use of multiple drugs which can contribute to postural hypotension.1 Prolonged periods of bed rest and large varicose veins also contribute to postural hypotension. The prevalence of postural hypotension with a systolic blood pressure decrease ⩾220 mm Hg after standing has been reported to range from 11 to 24\% in persons {\textgreater}61 years of age.2–6},
	number = {4},
